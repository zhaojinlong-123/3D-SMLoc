<launch>
    <!--  get images -->
    <node name="pubimage" pkg="semantic_3d_avp" type="pubimage" output="screen">
    </node>

    <!-- load Parameters -->
    <rosparam file="$(find semantic_3d_avp)/config/map_zjg2.yaml" command="load" />
   
    <!-- generate point cloud from RGB image of multi cameras -->
    <node name="pointCloudFromRGB" pkg="semantic_3d_avp" type="pointCloudFromRGB" output="screen">
    </node>

　　 <!--  localization and mapping -->
    <node name="mapping" pkg="semantic_3d_avp" type="mapping" output="screen">
    </node>

    <!--  loop closure -->
    <!--<node name="PGO" pkg="semantic_3d_avp" type="PGO" output="screen">
    </node> -->

    <node name="object_map_node" pkg="semantic_3d_avp" type="object_map_node" output="screen">
    </node>

</launch>